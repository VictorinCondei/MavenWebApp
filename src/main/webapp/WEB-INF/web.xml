<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>OkdServletOne</servlet-name>
  	<display-name>OkdServletOne</display-name>
  	<description></description>
  	<servlet-class>net.javaMavenWebServlets.OkdServletOne</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>OkdPatternServlet</servlet-name>
  	<display-name>OkdPatternServlet</display-name>
  	<description></description>
  	<servlet-class>net.javaMavenWebServlets.OkdPatternServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>OkdServletOne</servlet-name>
  	<url-pattern>/OkdServletOne</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>OkdPatternServlet</servlet-name>
  	<url-pattern>/OkdPatternServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
